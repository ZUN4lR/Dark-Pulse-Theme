{
	"name": "dark-pulse-theme",
	"displayName": "Dark Pulse Theme",
	"description": "An epic theme for Visual Studio Code designed to keep your code glowing with precision.",
	"version": "1.0.0",
	"publisher": "ZUN4lR",
	"author": "Muhammad Zunair",
	"icon": "logo.png",
	"galleryBanner": {
		"color": "#11131C",
		"theme": "dark"
	},
	"homepage": "https://github.com/ZUN4lR/Dark-Pulse-Theme",
	"repository": {
		"type": "git",
		"url": "https://github.com/ZUN4lR/Dark-Pulse-Theme.git"
	},
	"bugs": {
		"url": "https://github.com/ZUN4lR/Dark-Pulse-Theme/issues"
	},
	"engines": {
		"node": ">=12",
		"vscode": "^1.51.0"
	},
	"categories": ["Themes", "Other"],
	"keywords": ["VSCode", "Themes"],
	"activationEvents": ["onStartupFinished"],
	"extensionKind": ["ui", "workspace"],
	"main": "./build/dark.pulse.theme.config",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "Dark Pulse Theme",
			"properties": {
				"darkPulseTheme.accent": {
					"type": "string",
					"default": "Blue",
					"enum": [
						"Acid Lime",
						"Blue",
						"Breaking Bad",
						"Bright Teal",
						"Cyan",
						"Graphite",
						"Indigo",
						"Lime",
						"Orange",
						"Pink",
						"Purple",
						"Red",
						"Sky",
						"Tomato",
						"Teal",
						"Yellow"
					],
					"description": "Current accent color selected",
					"scope": "window"
				}
			}
		},
			"themes": [
			{
				"label": "Pulse Theme",
				"path": "./build/themes/Pulse-Theme-Default.json",
				"uiTheme": "vs-dark"
			},
			{
				"label": "Pulse Theme High Contrast",
				"path": "./build/themes/Pulse-Theme-Default-High-Contrast.json",
				"uiTheme": "vs-dark"
			},
			{
				"label": "Pulse Theme Darker",
				"path": "./build/themes/Pulse-Theme-Darker.json",
				"uiTheme": "vs-dark"
			},
			{
				"label": "Pulse Theme Darker High Contrast",
				"path": "./build/themes/Pulse-Theme-Darker-High-Contrast.json",
				"uiTheme": "vs-dark"
			},
			{
				"label": "Pulse Theme Palenight",
				"path": "./build/themes/Pulse-Theme-Palenight.json",
				"uiTheme": "vs-dark"
			},
			{
				"label": "Pulse Theme Palenight High Contrast",
				"path": "./build/themes/Pulse-Theme-Palenight-High-Contrast.json",
				"uiTheme": "vs-dark"
			},
			{
				"label": "Pulse Theme Ocean",
				"path": "./build/themes/Pulse-Theme-Ocean.json",
				"uiTheme": "vs-dark"
			},
			{
				"label": "Pulse Theme Ocean High Contrast",
				"path": "./build/themes/Pulse-Theme-Ocean-High-Contrast.json",
				"uiTheme": "vs-dark"
			},
			{
				"label": "Pulse Theme Deepforest",
				"path": "./build/themes/Pulse-Theme-Deepforest.json",
				"uiTheme": "vs-dark"
			},
			{
				"label": "Pulse Theme DeepForest High Contrast",
				"path": "./build/themes/Pulse-Theme-Deepforest-High-Contrast.json",
				"uiTheme": "vs-dark"
			},
			{
				"label": "Pulse Theme Lighter",
				"path": "./build/themes/Pulse-Theme-Lighter.json",
				"uiTheme": "vs"
			},
			{
				"label": "Pulse Theme Lighter High Contrast",
				"path": "./build/themes/Pulse-Theme-Lighter-High-Contrast.json",
				"uiTheme": "vs"
			}
		]
	},
	"dependencies": {
		"@sanity/client": "2.1.0"
	},
	"devDependencies": {
		"typescript": "4.1.3",
		"eslint": "7.2.0"
	}
}
